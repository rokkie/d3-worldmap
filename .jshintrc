{
  "maxerr"   : 100,
  "debug"    : false,
  "devel"    : true,
  "node"     : false,
  "browser"  : true,
  "esversion": 6,
  "asi"      : false,
  "bitwise"  : true,
  "boss"     : false,
  "camelcase": true,
  "curly"    : true,
  "eqeqeq"   : true,
  "eqnull"   : false,
  "evil"     : false,
  "expr"     : false,
  "forin"    : true,
  "latedef"  : false,
  "loopfunc" : false,
  "noarg"    : true,
  "quotmark" : "single",
  "scripturl": false,
  "shadow"   : "inner",
  "supernew" : false,
  "undef"    : true,
  "unused"   : "vars",
  "nonew"    : true,
  "plusplus" : false,
  "indent"   : 2,
  "globals"  : {
    "Promise" : true,
    "d3"      : true,
    "topojson": true,
    "Utils"   : true
  }
}
